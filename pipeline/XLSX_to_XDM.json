{
	"name": "XLSX_to_XDM",
	"properties": {
		"activities": [
			{
				"name": "Get New Load ID",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "declare\n  @v_load_id int\n  begin\n   exec semarchy_repository.dbo.get_new_loadid\n    @return_value_argument=@v_load_id OUTPUT,\n    @p_data_location_name = 'CustomerB2c', /* Data Location Name (as in the UI) */\n    @p_program_name = 'ADF',\t\t/*  Informational. Identifies the Middleware */\n    @p_load_description = N'Blob XSLS to XDM',/* Informational. Describes the load. */\n    \t\t\t\t\t\t/* Note the N to allow accented characters */\n    @p_username = 'semadmin'\t\t/* User initializing the load. */\n    select @v_load_id as loadid\nend",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "SemarchyRepository",
						"type": "DatasetReference"
					}
				}
			},
			{
				"name": "Set Load ID",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Get New Load ID",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "LoadID",
					"value": {
						"value": "\"@activity('Get New Load ID').output.firstRow.loadid\"",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Submit Load",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "[dbo].[SUBMIT_LOAD]",
					"storedProcedureParameters": {
						"P_INTEGRATION_JOB_NAME": {
							"value": "INTEGRATE_ALL",
							"type": "String"
						},
						"P_LOAD_ID": {
							"value": {
								"value": "@variables('LoadID')",
								"type": "Expression"
							},
							"type": "Decimal"
						},
						"P_USERNAME": {
							"value": "semadmin",
							"type": "String"
						},
						"return_value_argument": {
							"value": null,
							"type": "Decimal"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "SemarchyRepository",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Set Load ID",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"additionalColumns": [
							{
								"name": "LoadID",
								"value": {
									"value": "@activity('Get New Load ID').output.firstRow.loadid",
									"type": "Expression"
								}
							},
							{
								"name": "ClassName",
								"value": "Nickname"
							}
						],
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Given Name",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "GIVEN_NAME",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "Top Nickname",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "TOP_NICKNAME",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "All Nicknames",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "ALL_NICKNAMES",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "LoadID",
									"type": "String"
								},
								"sink": {
									"name": "B_LOADID",
									"type": "Decimal",
									"physicalType": "decimal",
									"scale": 0,
									"precision": 38
								}
							},
							{
								"source": {
									"name": "ClassName",
									"type": "String"
								},
								"sink": {
									"name": "B_CLASSNAME",
									"type": "String",
									"physicalType": "nvarchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "NicknamesXLSX",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Nicknames_SA_Table",
						"type": "DatasetReference"
					}
				]
			}
		],
		"variables": {
			"LoadID": {
				"type": "String"
			}
		},
		"annotations": []
	}
}